<h4>List of all articles</h4>
<p> 
	<%= link_to "Create new Article", new_article_path %> 
</p>
<table>
	<tr>
		<th>Title</th>
		<th>Text</th>

	</tr>
	<% @articles.each do |article| %>
		<tr>
			<td><%= article.title%> </td>
			<td><%= article.text%> </td>
			<td><%= link_to "Edit", edit_article_path(article)%></td>
			<td><%= link_to "Show", article_path(article)%></td>
		</tr>
	<% end %>

</table>
